			phandle = <0x75>;

			nfc@c0000000 {
				compatible = "atmel,sama5d3-nfc";
				#address-cells = <0x01>;
				#size-cells = <0x01>;
				reg = <0xc0000000 0x8000000 0xf8014000 0x70 0x100000 0x100000>;
				clocks = <0x0f>;
				atmel,write-by-sram;
			};
		};

		sdio-host@a0000000 {
			compatible = "atmel,sama5d2-sdhci";
			reg = <0xa0000000 0x300>;
			interrupts = <0x1f 0x04 0x00>;
			clocks = <0x10 0x11 0x12>;
			clock-names = "hclock\0multclk\0baseclk";
			status = "disabled";
			phandle = <0x76>;
		};

		sdio-host@b0000000 {
			compatible = "atmel,sama5d2-sdhci";
			reg = <0xb0000000 0x300>;
			interrupts = <0x20 0x04 0x00>;
			clocks = <0x13 0x14 0x12>;
			clock-names = "hclock\0multclk\0baseclk";
			status = "okay";
			bus-width = <0x04>;
			pinctrl-names = "default";
			pinctrl-0 = <0x15>;
			phandle = <0x77>;
		};

		nfc-io@c0000000 {
			compatible = "atmel,sama5d3-nfc-io\0syscon";
			reg = <0xc0000000 0x8000000>;
			phandle = <0x0d>;
		};

		apb {
			compatible = "simple-bus";
			#address-cells = <0x01>;
			#size-cells = <0x01>;
			ranges;

			hlcdc@f0000000 {
				compatible = "atmel,sama5d2-hlcdc";
				reg = <0xf0000000 0x2000>;
				interrupts = <0x2d 0x04 0x00>;
				clocks = <0x16 0x17 0x18>;
