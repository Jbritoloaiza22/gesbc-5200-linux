				hlcdc-display-controller {
					compatible = "atmel,hlcdc-display-controller";
					#address-cells = <0x01>;
					#size-cells = <0x00>;

					port@0 {
						#address-cells = <0x01>;
						#size-cells = <0x00>;
						reg = <0x00>;
					};
				};

				hlcdc-pwm {
					compatible = "atmel,hlcdc-pwm";
					#pwm-cells = <0x03>;
					phandle = <0x79>;
				};
			};

			isc@f0008000 {
				compatible = "atmel,sama5d2-isc";
				reg = <0xf0008000 0x4000>;
				interrupts = <0x2e 0x04 0x05>;
				clocks = <0x19 0x1a 0x1b>;
				clock-names = "hclock\0iscck\0gck";
				#clock-cells = <0x00>;
				clock-output-names = "isc-mck";
