/dts-v1/;

/ {
	#address-cells = <0x01>;
	#size-cells = <0x01>;
	model = "Glomation GESBC-5200";
	compatible = "atmel,sama5d27-som1-ek\0atmel,sama5d27-som1\0atmel,sama5d27\0atmel,sama5d2\0atmel,sama5";
	interrupt-parent = <0x01>;

	chosen {
		stdout-path = "serial0:115200n8";
		bootargs = "console=ttyS0,115200 root=/dev/mmcblk0p2 rw rootwait";
	};

	aliases {
		serial0 = "/ahb/apb/serial@f8020000";
		serial1 = "/ahb/apb/flexcom@fc018000/serial@200";
		tcb0 = "/ahb/apb/timer@f800c000";
		tcb1 = "/ahb/apb/timer@f8010000";
		i2s0 = "/ahb/apb/i2s@f8050000";
		i2s1 = "/ahb/apb/i2s@fc04c000";
		serial2 = "/ahb/apb/flexcom@f8038000/serial@200";
		serial3 = "/ahb/apb/flexcom@fc014000/serial@200";
		serial4 = "/ahb/apb/flexcom@fc010000/serial@200";
		i2c1 = "/ahb/apb/i2c@fc028000";
	};

	memory {
		device_type = "memory";
		reg = <0x20000000 0x20000000>;
	};

	cpus {
		#address-cells = <0x01>;
		#size-cells = <0x00>;

		cpu@0 {
			device_type = "cpu";
			compatible = "arm,cortex-a5";
			reg = <0x00>;
			next-level-cache = <0x02>;
		};
	};

	pmu {
		compatible = "arm,cortex-a5-pmu";
		interrupts = <0x02 0x04 0x00>;
	};

	etb {
		compatible = "arm,coresight-etb10\0arm,primecell";
		reg = <0x740000 0x1000>;
		clocks = <0x03>;
		clock-names = "apb_pclk";

		port {

			endpoint {
				slave-mode;
				remote-endpoint = <0x04>;
				phandle = <0x05>;
			};
		};
	};

	etm {
		compatible = "arm,coresight-etm3x\0arm,primecell";
		reg = <0x73c000 0x1000>;
		clocks = <0x03>;
		clock-names = "apb_pclk";

		port {

			endpoint {
				remote-endpoint = <0x05>;
				phandle = <0x04>;
			};
		};
	};

	clocks {

		slow_xtal {
			compatible = "fixed-clock";
			#clock-cells = <0x00>;
			clock-frequency = <0x8000>;
			phandle = <0x4d>;
		};

		main_xtal {
			compatible = "fixed-clock";
			#clock-cells = <0x00>;
			clock-frequency = <0x16e3600>;
			phandle = <0x21>;
		};

		pioB_clk {
			compatible = "fixed-clock";
			#clock-cells = <0x00>;
			clock-frequency = <0x5f5e100>;
			phandle = <0xb0>;
		};
	};

	sram@200000 {
		compatible = "mmio-sram";
		reg = <0x200000 0x20000>;
		phandle = <0x6f>;
	};

	ahb {
		compatible = "simple-bus";
		#address-cells = <0x01>;
		#size-cells = <0x01>;
		ranges;

		sram@100000 {
			compatible = "mmio-sram";
			no-memory-wc;
			reg = <0x100000 0x2400>;
			phandle = <0x0c>;
		};

		gadget@300000 {
			#address-cells = <0x01>;
			#size-cells = <0x00>;
			compatible = "atmel,sama5d3-udc";
			reg = <0x300000 0x100000 0xfc02c000 0x400>;
			interrupts = <0x2a 0x04 0x02>;
			clocks = <0x06 0x07>;
			clock-names = "pclk\0hclk";
			status = "disabled";
			phandle = <0x70>;

			ep@0 {
				reg = <0x00>;
				atmel,fifo-size = <0x40>;
				atmel,nb-banks = <0x01>;
			};

			ep@1 {
				reg = <0x01>;
				atmel,fifo-size = <0x400>;
				atmel,nb-banks = <0x03>;
				atmel,can-dma;
				atmel,can-isoc;
			};

			ep@2 {
				reg = <0x02>;
				atmel,fifo-size = <0x400>;
				atmel,nb-banks = <0x03>;
				atmel,can-dma;
				atmel,can-isoc;
			};

			ep@3 {
				reg = <0x03>;
				atmel,fifo-size = <0x400>;
				atmel,nb-banks = <0x02>;
				atmel,can-dma;
				atmel,can-isoc;
			};

			ep@4 {
				reg = <0x04>;
				atmel,fifo-size = <0x400>;
				atmel,nb-banks = <0x02>;
				atmel,can-dma;
				atmel,can-isoc;
			};

			ep@5 {
				reg = <0x05>;
				atmel,fifo-size = <0x400>;
				atmel,nb-banks = <0x02>;
				atmel,can-dma;
				atmel,can-isoc;
			};

			ep@6 {
				reg = <0x06>;
				atmel,fifo-size = <0x400>;
				atmel,nb-banks = <0x02>;
				atmel,can-dma;
				atmel,can-isoc;
			};

			ep@7 {
				reg = <0x07>;
				atmel,fifo-size = <0x400>;
				atmel,nb-banks = <0x02>;
				atmel,can-dma;
				atmel,can-isoc;
			};

			ep@8 {
				reg = <0x08>;
				atmel,fifo-size = <0x400>;
				atmel,nb-banks = <0x02>;
				atmel,can-isoc;
			};

			ep@9 {
				reg = <0x09>;
				atmel,fifo-size = <0x400>;
				atmel,nb-banks = <0x02>;
				atmel,can-isoc;
			};

			ep@10 {
				reg = <0x0a>;
				atmel,fifo-size = <0x400>;
				atmel,nb-banks = <0x02>;
				atmel,can-isoc;
			};

			ep@11 {
				reg = <0x0b>;
				atmel,fifo-size = <0x400>;
				atmel,nb-banks = <0x02>;
				atmel,can-isoc;
			};

			ep@12 {
				reg = <0x0c>;
				atmel,fifo-size = <0x400>;
				atmel,nb-banks = <0x02>;
				atmel,can-isoc;
			};

			ep@13 {
				reg = <0x0d>;
				atmel,fifo-size = <0x400>;
				atmel,nb-banks = <0x02>;
				atmel,can-isoc;
			};

			ep@14 {
				reg = <0x0e>;
				atmel,fifo-size = <0x400>;
				atmel,nb-banks = <0x02>;
				atmel,can-isoc;
			};

			ep@15 {
				reg = <0x0f>;
				atmel,fifo-size = <0x400>;
				atmel,nb-banks = <0x02>;
				atmel,can-isoc;
			};
		};

		ohci@400000 {
			compatible = "atmel,at91rm9200-ohci\0usb-ohci";
			reg = <0x400000 0x100000>;
			interrupts = <0x29 0x04 0x02>;
			clocks = <0x08 0x08 0x09>;
			clock-names = "ohci_clk\0hclk\0uhpck";
			status = "okay";
			num-ports = <0x03>;
			phandle = <0x71>;
		};

		ehci@500000 {
			compatible = "atmel,at91sam9g45-ehci\0usb-ehci";
			reg = <0x500000 0x100000>;
			interrupts = <0x29 0x04 0x02>;
			clocks = <0x07 0x08>;
			clock-names = "usb_clk\0ehci_clk";
			status = "okay";
			phandle = <0x72>;
		};

		cache-controller@a00000 {
			compatible = "arm,pl310-cache";
			reg = <0xa00000 0x1000>;
			interrupts = <0x3f 0x04 0x04>;
			cache-unified;
			cache-level = <0x02>;
			phandle = <0x02>;
		};

		ebi@10000000 {
			compatible = "atmel,sama5d3-ebi";
			#address-cells = <0x02>;
			#size-cells = <0x01>;
			atmel,smc = <0x0a>;
			reg = <0x10000000 0x10000000 0x60000000 0x30000000>;
			ranges = <0x00 0x00 0x10000000 0x10000000 0x01 0x00 0x60000000 0x10000000 0x02 0x00 0x70000000 0x10000000 0x03 0x00 0x80000000 0x10000000>;
			clocks = <0x03>;
			status = "okay";
			pinctrl-names = "default";
			pinctrl-0 = <0x0b>;
			phandle = <0x73>;

			nand-controller {
				compatible = "atmel,sama5d3-nand-controller";
				atmel,nfc-sram = <0x0c>;
				atmel,nfc-io = <0x0d>;
				ecc-engine = <0x0e>;
				#address-cells = <0x02>;
				#size-cells = <0x01>;
				ranges;
				status = "okay";
				phandle = <0x74>;

				nand@3 {
					reg = <0x03 0x00 0x02>;
					atmel,rb = <0x00>;
					nand-bus-width = <0x08>;
					nand-ecc-mode = "hw";
					nand-on-flash-bbt;
					label = "atmel_nand";

					partitions {
						compatible = "fixed-partitions";
						#address-cells = <0x01>;
						#size-cells = <0x01>;

						at91bootstrap@0 {
							label = "bootstrap";
							reg = <0x00 0x40000>;
						};

						dtb@180000 {
							label = "device tree";
							reg = <0x180000 0x80000>;
						};

						kernel@200000 {
							label = "kernel";
							reg = <0x200000 0x600000>;
						};

						rootfs@800000 {
							label = "rootfs";
							reg = <0x800000 0x7800000>;
						};
					};
				};
			};
		};

		nand@80000000 {
			compatible = "atmel,sama5d2-nand";
			#address-cells = <0x01>;
			#size-cells = <0x01>;
			ranges;
			reg = <0x80000000 0x8000000 0xf8014070 0x490 0xf8014500 0x200 0x40000 0x18000>;
			interrupts = <0x11 0x04 0x06>;
			atmel,nand-addr-offset = <0x15>;
			atmel,nand-cmd-offset = <0x16>;
			atmel,nand-has-dma;
			atmel,has-pmecc;
			atmel,pmecc-lookup-table-offset = <0x00 0x8000>;
